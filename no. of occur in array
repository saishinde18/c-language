#include<stdio.h>
int main()
{
	int a[100],n,i,c=0,num;
	printf("enter limit::");
	scanf("%d",&n);
	printf("enter numbers::");
	for(i=0;i<n;i++)
	{
		scanf("%d",&a[i]);
	}
		printf("enter count of number::");
		scanf("%d",&num);
		for(i=0;i<n;i++)
    	{
    		if(a[i]==num)
    		{
    			c++;
			}
		}
		printf("number of occurance=%d",c);
    }

